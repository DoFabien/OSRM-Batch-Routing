# High performance configuration for OSRM Batch Routing
# Use this for maximum speed when you have sufficient resources

# Batch processing
BATCH_SIZE=100

# OSRM parallel processing - 100 concurrent requests
OSRM_MAX_CONCURRENT=100
OSRM_REQUEST_DELAY=0

# Memory limits - allow large jobs
MAX_JOBS_KEPT=10
IMMEDIATE_CLEANUP=false

# Server timeouts - allow longer processing
JOB_TIMEOUT=7200000

# File processing
FILE_CLEANUP_INTERVAL=24

# Logging
LOG_LEVEL=info